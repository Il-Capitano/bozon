function print(str: str) -> void;
function println(str: str) -> void;
function print_int32(n: int32) -> void;

function factorial(n: int32) -> int32
{
	let res = 1;
	while (!(n == 1))
	{
		res = res * n;
		n = n - 1;
	}
	return res;
}

