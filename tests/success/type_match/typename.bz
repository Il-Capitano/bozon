function test1(T: typename);
function test2(T: **typename);
function test3(T: [4: typename]);
function test4(T: [??: *typename]);
function test5(T: [: typename]);
function test6(T: [typename, typename]);
function test7(T: [...typename]);
function test8(T: [typename, ...typename]);
function test9(T: [typename, i32]);

function test()
{
	test1(i32);
	test1(mut i32);
	test1(*i32);
	test1([: mut i32]);
	test1(&[10: i32]);
	test1(move [str, float32, [[3: i8], usize]]);

	test2(**i32);
	test2(**mut *[float32, float64]);

	test3([4: i32]);

	test4([3: *i32]);
	test4([4: *i32]);
	test4([2: *mut i32]);
	test4([8: **mut i32]);

	test5([: i32]);
	test5([: mut i32]);
	test5([: **i32]);
	test5([: mut [4: i32]]);

	test6([i32, float32]);
	test6([str, char]);
	test6([[: mut str], [i8, u64, *i32]]);

	test7([i32]);
	test7([i32, float32]);
	test7([i32, float32, str]);
	test7([i32, float32, str, char]);

	test8([i32]);
	test8([i32, float32]);
	test8([i32, float32, str]);
	test8([i32, float32, str, char]);

	test9([i32, i32]);
	test9([str, i32]);
}
