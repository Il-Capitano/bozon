function test1([...elems]) -> [sizeof ...elems: i32]
{
	return [sizeof ...elems: i32]();
}

function test()
{
	let t = [i32, f32, str, f64, usize]();
	test1(t);
	static_assert(typeof test1(t) == [5: i32]);

	let arr = [10: i32]();
	test1(arr);
	static_assert(typeof test1(arr) == [10: i32]);

	let arr = [10, 20: i32]();
	test1(arr);
	static_assert(typeof test1(arr) == [10: i32]);
}
