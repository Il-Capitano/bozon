function compare_test(a, b, c)
{
	if (!(a < b) || !(a < c) || !(b < c)) unreachable;
	if (b < a || c < a || c < b) unreachable;

	if (!(b > a) || !(c > a) || !(c > b)) unreachable;
	if (a > b || a > c || b > c) unreachable;

	if (!(a <= b) || !(a <= c) || !(b <= c) || !(a <= a)) unreachable;
	if (b <= a || c <= a || c <= b) unreachable;

	if (!(b >= a) || !(c >= a) || !(c >= b) || !(a >= a)) unreachable;
	if (a >= b || a >= c || b >= c) unreachable;

	if (!(a == a) || !(b == b) || !(c == c)) unreachable;
	if (a == b || a == c || b == c) unreachable;

	if (a != a || b != b || c != c) unreachable;
	if (!(a != b) || !(a != c) || !(b != c)) unreachable;
}

function compare_test_signed(intN: typename)
{
	let a: intN = -99;
	let b: intN = 0;
	let c: intN = 99;

	compare_test(a, b, c);
}

function compare_test_unsigned(uintN: typename)
{
	let a: uintN = 0;
	let b: uintN = 99;
	let c: uintN = 199;

	compare_test(a, b, c);
}

function compare_test_float(floatN: typename)
{
	compare_test(-24.3 as floatN, 0.0 as floatN, 3.53e8 as floatN);

	let nan = ((0.0 / 0.0)) as floatN;
	let inf = ((1.0 / 0.0)) as floatN;

	if (nan < nan) unreachable;
	if (inf < nan) unreachable;
	if (3.83532 as floatN < nan) unreachable;

	if (nan > nan) unreachable;
	if (inf > nan) unreachable;
	if (3.83532 as floatN > nan) unreachable;

	if (nan <= nan) unreachable;
	if (inf <= nan) unreachable;
	if (3.83532 as floatN <= nan) unreachable;

	if (nan >= nan) unreachable;
	if (inf >= nan) unreachable;
	if (3.83532 as floatN >= nan) unreachable;

	if (nan == nan) unreachable;
	if (inf == nan) unreachable;
	if (3.83532 as floatN == nan) unreachable;

	if (!(nan != nan)) unreachable;
	if (!(inf != nan)) unreachable;
	if (!(3.83532 as floatN != nan)) unreachable;
}

function main()
{
	compare_test_signed(int8);
	compare_test_signed(int16);
	compare_test_signed(int32);
	compare_test_signed(int64);

	compare_test_unsigned(uint8);
	compare_test_unsigned(uint16);
	compare_test_unsigned(uint32);
	compare_test_unsigned(uint64);

	compare_test('a', 'b', 'c');

	compare_test_float(float32);
	compare_test_float(float64);

	let a = [10: int32]();
	compare_test(&a[0], &a[3], &a[9]);

	let a = false;
	let b = true;

	if (!(a == a)) unreachable;
	if (!(b == b)) unreachable;
	if (a == b) unreachable;
	if (!(a != b)) unreachable;

	if (!(false == a)) unreachable;
	if (false == b) unreachable;
	if (!(false != b)) unreachable;

	if (!(true == b)) unreachable;
	if (a == true) unreachable;
	if (!(a != true)) unreachable;
}
