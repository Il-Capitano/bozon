struct S
{
	.i: i32;
	.j: usize;
	.x: f32;
	.y: f64;
	.a: [20: u32];
	.s: str;
}

function main()
{
	{
		static_assert(__builtin_is_trivially_copy_constructible(i32));
		let n: i32 = 42;
		let n_copy = n;
		if (n != n_copy) unreachable;
	}

	{
		static_assert(__builtin_is_trivially_copy_constructible(f64));
		let x: f64 = 42.42;
		let x_copy = x;
		if (x != x_copy) unreachable;
	}

	{
		static_assert(__builtin_is_trivially_copy_constructible(str));
		let s: str = "hello";
		let s_copy = s;
		if (s != s_copy) unreachable;
	}

	{
		static_assert(__builtin_is_trivially_copy_constructible(S));
		let s: S = S[
			-99,
			100,
			3.14159265f32,
			2.71828,
			__builtin_create_initialized_array([20: u32], 42u32),
			"hello",
		];
		let s_copy = s;
		if (s.i != s_copy.i) unreachable;
		if (s.j != s_copy.j) unreachable;
		if (s.x != s_copy.x) unreachable;
		if (s.y != s_copy.y) unreachable;
		if (s.a != s_copy.a) unreachable;
		if (s.s != s_copy.s) unreachable;
	}
}
