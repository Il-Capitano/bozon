function main()
{
	let i8  = 10i8;
	let i16 = 10i16;
	let i32 = 10i32;
	let i64 = 10i64;
	let f32 = 1.3f32;
	let f64 = 1.3f64;

	if (-i8  != -10) unreachable;
	if (-i16 != -10) unreachable;
	if (-i32 != -10) unreachable;
	if (-i64 != -10) unreachable;
	if (-f32 != -1.3f32) unreachable;
	if (-f64 != -1.3f64) unreachable;

	let i8_min  = (((1u8  <<  7) as ::i8));
	let i16_min = (((1u16 << 15) as ::i16));
	let i32_min = (((1u32 << 31) as ::i32));
	let i64_min = (((1u64 << 63) as ::i64));

	if (-i8_min  != i8_min) unreachable;
	if (-i16_min != i16_min) unreachable;
	if (-i32_min != i32_min) unreachable;
	if (-i64_min != i64_min) unreachable;

	let f32_inf = ((1.0f32 / 0.0f32));
