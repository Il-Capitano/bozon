function main()
{
	let s = "";
	mut ps: [2: ?*uint8] = [ null, null ];
	static_assert(sizeof ps <= sizeof s);
	__builtin_memcpy(&ps, &s, sizeof ps);
	if (ps[1] != __builtin_str_end_ptr(s)) unreachable;

	let s = "hello";
	mut ps: [2: ?*uint8] = [ null, null ];
	static_assert(sizeof ps <= sizeof s);
	__builtin_memcpy(&ps, &s, sizeof ps);
	if (ps[1] != __builtin_str_end_ptr(s)) unreachable;

	let a = [10: uint8]();
	let s = __builtin_str_from_ptrs(&a[0], &a[0] + 10);
	if (__builtin_str_end_ptr(s) != &a[0] + 10) unreachable;
}
