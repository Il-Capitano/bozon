function check(intN: typename, r)
{
	if (__builtin_bit_cast(intN, r) != __builtin_integer_range_from_begin_value(r)) unreachable;
}

function main()
{
	let begin = 3i8;
	let r = __builtin_integer_range_from_i8(begin);
	check(i8, r);
	let begin = -33i8;
	let r = __builtin_integer_range_from_i8(begin);
	check(i8, r);
	let begin = 0i8;
	let r = __builtin_integer_range_from_i8(begin);
	check(i8, r);

	let begin = 3i16;
	let r = __builtin_integer_range_from_i16(begin);
	check(i16, r);
	let begin = -33i16;
	let r = __builtin_integer_range_from_i16(begin);
	check(i16, r);
	let begin = 0i16;
	let r = __builtin_integer_range_from_i16(begin);
	check(i16, r);

	let begin = 3i32;
	let r = __builtin_integer_range_from_i32(begin);
	check(i32, r);
	let begin = -33i32;
	let r = __builtin_integer_range_from_i32(begin);
	check(i32, r);

	let begin = 3i64;
	let r = __builtin_integer_range_from_i64(begin);
	check(i64, r);
	let begin = -33i64;
	let r = __builtin_integer_range_from_i64(begin);
	check(i64, r);

	let begin = 3u8;
	let r = __builtin_integer_range_from_u8(begin);
	check(u8, r);

	let begin = 3u16;
	let r = __builtin_integer_range_from_u16(begin);
	check(u16, r);

	let begin = 3u32;
	let r = __builtin_integer_range_from_u32(begin);
	check(u32, r);

	let begin = 3u64;
	let r = __builtin_integer_range_from_u64(begin);
	check(u64, r);
}
