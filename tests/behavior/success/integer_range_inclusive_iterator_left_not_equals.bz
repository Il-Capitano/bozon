	let r = __builtin_integer_range_inclusive_i16(begin, end);
	check(i16, r);
	let begin = 0i16;
	let end = 0i16;
	let r = __builtin_integer_range_inclusive_i16(begin, end);
	check(i16, r);

	let begin = 3i32;
	let end = 122i32;
	let r = __builtin_integer_range_inclusive_i32(begin, end);
	check(i32, r);
	let begin = -33i32;
	let end = 72i32;
	let r = __builtin_integer_range_inclusive_i32(begin, end);
	check(i32, r);
	let begin = 0i32;
	let end = 0i32;
	let r = __builtin_integer_range_inclusive_i32(begin, end);
	check(i32, r);

	let begin = 3i64;
	let end = 122i64;
	let r = __builtin_integer_range_inclusive_i64(begin, end);
	check(i64, r);
	let begin = -33i64;
	let end = 72i64;
	let r = __builtin_integer_range_inclusive_i64(begin, end);
	check(i64, r);
	let begin = 0i64;
	let end = 0i64;
	let r = __builtin_integer_range_inclusive_i64(begin, end);
	check(i64, r);

	let begin = 3u8;
	let end = 122u8;
	let r = __builtin_integer_range_inclusive_u8(begin, end);
	check(u8, r);
	let begin = 0u8;
	let end = 0u8;
	let r = __builtin_integer_range_inclusive_u8(begin, end);
	check(u8, r);

	let begin = 3u16;
	let end = 122u16;
	let r = __builtin_integer_range_inclusive_u16(begin, end);
	check(u16, r);
	let begin = 0u16;
	let end = 0u16;
	let r = __builtin_integer_range_inclusive_u16(begin, end);
	check(u16, r);

	let begin = 3u32;
	let end = 122u32;
	let r = __builtin_integer_range_inclusive_u32(begin, end);
	check(u32, r);
	let begin = 0u32;
	let end = 0u32;
	let r = __builtin_integer_range_inclusive_u32(begin, end);
