import std::libc::stdio;
import meta::comptime;

export function print_stdout(s: str)
{
	if (meta::is_comptime())
	{
		meta::comptime_print(s);
	}
	else
	{
		libc::fwrite(s.begin_ptr(), 1, s.size(), libc::stdout());
	}
}

export function print_stderr(s: str)
{
	if (meta::is_comptime())
	{
		meta::comptime_print(s);
	}
	else
	{
		libc::fwrite(s.begin_ptr(), 1, s.size(), libc::stderr());
	}
}
